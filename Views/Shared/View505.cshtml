@model View505ViewModel
@{
    ViewData["Title"] = "Error";
}

<h1 class="text-danger">505 Error.</h1>

@if (Model != null)
{
    <p>
        <strong>Status Code:</strong> <code>@Model.StatusCode</code>
    </p>
    <p>
        <strong>HTTP Version Not Supported</strong>
        <strong>The server does not support the HTTP protocol version used in the request.</strong>
    </p>
}